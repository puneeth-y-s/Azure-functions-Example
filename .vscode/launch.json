{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Python Functions",
      "type": "python",
      "request": "attach",
      "port": 9091,
      "preLaunchTask": "func: host start",
      "justMyCode": false
    },
    {
      "name": "Debug hello_function",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/hello_function/__init__.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": ["1", "1"]
    }
  ]
}
